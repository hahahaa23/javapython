package ex0622;

public class BB {

	public static void main(String[] args) {
		
	Scanner scan = new Scanner(System.in);
	
	System.out.println("입력하세요");
	String aa = scan.nextline();
	System.out.println("aa = "+ aa);
	
	
	System.out.println("입력하세요");
	String bb = scan.nextline();
	System.out.println("aa = "+ bb);
	
	
	
		
	}
	
	
}
