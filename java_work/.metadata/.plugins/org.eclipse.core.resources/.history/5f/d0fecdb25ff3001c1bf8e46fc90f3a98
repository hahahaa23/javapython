package ex0622;

public class DD {
	
	public static void main(String[] args) {
		
		final int AA = 10;
		final String CC = "문자열";
		
		System.out.println("AA = "+ AA);
		System.out.println("CC = "+ CC);
		
		System.out.println("안녕하세요\n");
		System.out.println("안녕하세요\t박명회입니다.");
		System.out.println("\"안녕하세요\"");
		System.out.println("\'안녕하세요\'");
		System.out.println("\\안녕하세요\\");
		
		//final 은 값을 한 번만 가능
		//예) AA = 20
		
		
	}

}
